<?xml version="1.0" encoding="UTF-8"?>
<component xmlns="http://www.eclipse.org/wb/WBPComponent">
	<description>A button that displays text and an optional icon.</description>
	<!-- CREATION -->
	<creation>
		<source><![CDATA[new com.google.gwt.widget.client.TextButton("New button")]]></source>
		<x-content>New button</x-content>
	</creation>
	<!-- CONSTRUCTORS -->
	<constructors>
		<constructor>
			<parameter type="java.lang.String" property="setText(java.lang.String)"/>
		</constructor>
		<constructor>
			<parameter type="com.google.gwt.cell.client.TextButtonCell$Appearance"/>
			<parameter type="java.lang.String" property="setText(java.lang.String)"/>
		</constructor>
		<constructor>
			<parameter type="com.google.gwt.cell.client.TextButtonCell"/>
			<parameter type="java.lang.String" property="setText(java.lang.String)"/>
		</constructor>
	</constructors>
	<!-- PROPERTIES -->
	<properties-preferred names="decoration text"/>
	<property-tag name="text" tag="isText" value="true"/>
	<!-- PARAMETERS -->
	<parameters>
		<parameter name="x.double-click.listener">onClick</parameter>
	</parameters>
</component>
