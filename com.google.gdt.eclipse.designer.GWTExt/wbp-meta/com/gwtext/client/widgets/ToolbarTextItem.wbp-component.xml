<?xml version="1.0" encoding="UTF-8"?>
<component xmlns="http://www.eclipse.org/wb/WBPComponent">
	<model class="com.google.gdt.eclipse.designer.gwtext.model.widgets.ButtonInfo"/>
	<description>A simple class that renders text directly into a toolbar.</description>
	<!-- CREATION -->
	<creation>
		<source><![CDATA[new com.gwtext.client.widgets.ToolbarTextItem("New text item")]]></source>
	</creation>
	<!-- CONSTRUCTORS -->
	<constructors>
		<constructor>
			<parameter type="java.lang.String" property="setText(java.lang.String)"/>
		</constructor>
	</constructors>
	<!-- PROPERTIES -->
	<properties-preferred names="text"/>
	<properties-advanced names=""/>
	<property-tag name="text" tag="isText" value="true"/>
</component>
