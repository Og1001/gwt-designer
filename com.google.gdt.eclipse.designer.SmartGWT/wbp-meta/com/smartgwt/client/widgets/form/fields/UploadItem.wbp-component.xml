<?xml version="1.0" encoding="UTF-8"?>
<component xmlns="http://www.eclipse.org/wb/WBPComponent">
    <description>FormItem that creates an HTML 'input type=upload' control, with an interface that
        allows a user to pick a file from his machine to upload to the server.</description>
    <creation>
        <source><![CDATA[new com.smartgwt.client.widgets.form.fields.UploadItem("newUploadItem_%index%", "New UploadItem")]]></source>
    </creation>
    <!-- CONSTRUCTORS -->
    <constructors>
        <constructor>
            <parameter type="java.lang.String" property="setName(java.lang.String)"/>
        </constructor>
        <constructor>
            <parameter type="java.lang.String" property="setName(java.lang.String)"/>
            <parameter type="java.lang.String" property="setTitle(java.lang.String)"/>
        </constructor>
    </constructors>
    <!-- PROPERTIES -->
    <properties-preferred names=""/>
    <properties-advanced names="length setValue(java.lang.String)"/>
</component>
