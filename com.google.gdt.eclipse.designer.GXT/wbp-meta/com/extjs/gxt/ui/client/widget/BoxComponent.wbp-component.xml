<?xml version="1.0" encoding="UTF-8"?>
<component xmlns="http://www.eclipse.org/wb/WBPComponent">
	<model class="com.google.gdt.eclipse.designer.gxt.model.widgets.BoxComponentInfo"/>
	<!-- METHODS -->
	<methods>
		<method name="setSize">
			<parameter type="int"/>
			<parameter type="int"/>
		</method>
	</methods>
	<!-- PROPERTIES -->
	<properties-advanced names="shadow shadowOffset shadowPosition shim"/>
	<properties-advanced names="autoWidth autoHeight deferHeight"/>
	<property-tag name="height" tag="noDefaultValue" value="true"/>
	<!-- PARAMETERS -->
	<parameters>
		<!-- SplitBar -->
		<parameter name="simpleContainer.9">true</parameter>
		<parameter name="simpleContainer.9.component">com.extjs.gxt.ui.client.widget.SplitBar</parameter>
		<parameter name="simpleContainer_.association">%parent%.setMenu(%child%)</parameter>
		<!-- untyped events -->
		<parameter name="GXT.untyped.events: com.extjs.gxt.ui.client.widget.BoxComponent"><![CDATA[
			BoxComponentEvent Resize Move 
		]]></parameter>
	</parameters>
</component>
