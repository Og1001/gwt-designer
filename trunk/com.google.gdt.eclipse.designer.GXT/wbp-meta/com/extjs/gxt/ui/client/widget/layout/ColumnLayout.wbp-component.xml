<?xml version="1.0" encoding="UTF-8"?>
<component xmlns="http://www.eclipse.org/wb/WBPComponent">
	<model class="com.google.gdt.eclipse.designer.gxt.model.layout.ColumnLayoutInfo"/>
	<description>This layout positions and sizes the container's children in columns horizontally. Each
		component may specify its width in pixels or as percentage of the parent remaining width.</description>
	<!-- CREATION -->
	<creation>
		<source><![CDATA[new com.extjs.gxt.ui.client.widget.layout.ColumnLayout()]]></source>
	</creation>
	<!-- PARAMETERS -->
	<parameters>
		<parameter name="layout-data.has">true</parameter>
		<parameter name="layout-data.class">com.extjs.gxt.ui.client.widget.layout.ColumnData</parameter>
		<parameter name="layout-data.virtual">new %LDC%()</parameter>
		<!-- flow container -->
		<parameter name="flowContainer">true</parameter>
		<parameter name="flowContainer.horizontal">true</parameter>
	</parameters>
</component>
