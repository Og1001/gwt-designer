<?xml version="1.0" encoding="UTF-8"?>
<component xmlns="http://www.eclipse.org/wb/WBPComponent">
    <description>FormItem that shows a list of options, plus an "Other..." option that allows them
        to enter another value.</description>
    <creation>
        <source><![CDATA[new com.smartgwt.client.widgets.form.fields.SelectOtherItem("newSelectOtherItem_%index%", "New SelectOtherItem")]]></source>
    </creation>
    <!-- CONSTRUCTORS -->
    <constructors>
        <constructor>
            <parameter type="java.lang.String" property="setName(java.lang.String)"/>
        </constructor>
        <constructor>
            <parameter type="java.lang.String" property="setName(java.lang.String)"/>
            <parameter type="java.lang.String" property="setTitle(java.lang.String)"/>
        </constructor>
    </constructors>
    <!-- PROPERTIES -->
    <properties-preferred names="other* selectOtherPrompt"/>
    <properties-advanced names=""/>
</component>
