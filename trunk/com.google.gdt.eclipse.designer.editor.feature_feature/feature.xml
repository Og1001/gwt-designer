<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="com.google.gdt.eclipse.designer.editor.feature"
      label="%featureName"
      version="3.1.3"
      provider-name="%providerName">

   <description>
      GWT Designer Editor
   </description>

   <copyright>
      (c) Copyright Google, Inc. 2003 - ${build_year}.  All rights reserved.
   </copyright>

   <license url="%licenseURL">
      %license
   </license>

   <requires>
      <import feature="com.google.gdt.eclipse.designer.hosted.feature" version="0.0.0" match="greaterOrEqual"/>
      <import feature="org.eclipse.wb.core.feature" version="0.0.0" match="greaterOrEqual"/>
      <import feature="org.eclipse.wb.core.java.feature" version="0.0.0" match="greaterOrEqual"/>
      <!-- $if eclipse.version >= 3.5$ -->
      <import feature="org.eclipse.wb.core.xml.feature" version="0.0.0" match="greaterOrEqual"/>
      <!-- $endif$ -->
      <import feature="org.eclipse.wb.css.feature" version="0.0.0" match="greaterOrEqual"/>
   </requires>

   <plugin
         id="com.google.gdt.eclipse.designer"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <!-- $if eclipse.version >= 3.5$ -->
   <plugin
         id="com.google.gdt.eclipse.designer.UiBinder"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>
   <!-- $endif$ -->

</feature>
