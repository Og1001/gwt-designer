    <creation>
        <source><![CDATA[new com.smartgwt.client.widgets.form.fields.DateRangeItem("newDateRangeItem_%index%", "New DateRangeItem")]]></source>
    </creation>
    <!-- CONSTRUCTORS -->
    <constructors>
        <constructor>
            <parameter type="java.lang.String" property="setName(java.lang.String)"/>
        </constructor>
        <constructor>
            <parameter type="java.lang.String" property="setName(java.lang.String)"/>
            <parameter type="java.lang.String" property="setTitle(java.lang.String)"/>
        </constructor>
    </constructors>
    <!-- PROPERTIES -->
    <properties-preferred names=""/>
    <properties-advanced names="fieldName fromTitle toTitle"/>
    <properties-noDefaultValue names=""/>
    <property id="setAllowRelativeDates(java.lang.Boolean)">
        <defaultValue value="null"/>
    </property>
    <!-- PARAMETERS -->
    <parameters>
        <!-- simple container -->
        <parameter name="simpleContainer">false</parameter>
    </parameters>
</component>
