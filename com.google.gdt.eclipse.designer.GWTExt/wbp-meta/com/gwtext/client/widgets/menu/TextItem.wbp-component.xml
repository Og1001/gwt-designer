	</property>
	<!-- PARAMETERS -->
	<parameters>
		<!-- new -->
		<parameter name="liveComponent.no">true</parameter>
		<parameter name="liveComponent.forcedSize.width">100px</parameter>
		<parameter name="liveComponent.forcedSize.height">20px</parameter>
	</parameters>
