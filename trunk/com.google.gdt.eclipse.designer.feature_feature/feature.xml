<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="com.google.gdt.eclipse.designer.feature"
      label="%featureName"
      version="3.1.2"
      provider-name="%providerName"
      plugin="com.google.gdt.eclipse.designer"
      image="wb-icon.gif">

   <description>
      GWT Designer
   </description>

   <copyright>
      (c) Copyright Google, Inc. 2003 - ${build_year}.  All rights reserved.
   </copyright>

   <license url="%licenseURL">
      %license
   </license>

   <requires>
      <import feature="com.google.gdt.eclipse.designer.hosted.feature" version="0.0.0" match="greaterOrEqual"/>
      <import feature="org.eclipse.wb.core.feature" version="0.0.0" match="greaterOrEqual"/>
      <import feature="org.eclipse.wb.core.ui.feature" version="0.0.0" match="greaterOrEqual"/>
      <!-- $if eclipse.version >= 3.5$ -->
      <import feature="org.eclipse.wb.core.xml.feature" version="0.0.0" match="greaterOrEqual"/>
      <!-- $endif$ -->
      <import feature="org.eclipse.wb.css.feature" version="0.0.0" match="greaterOrEqual"/>
      <import feature="com.google.gdt.eclipse.designer.editor.feature" version="0.0.0" match="greaterOrEqual"/>
      <import feature="org.eclipse.wb.doc.user.feature" version="0.0.0" match="greaterOrEqual"/>
   </requires>

   <plugin
         id="com.google.gdt.eclipse.designer.GWTExt"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="com.google.gdt.eclipse.designer.doc.user"
         download-size="0"
         install-size="0"
         version="0.0.0"/>

   <plugin
         id="com.google.gdt.eclipse.designer.GXT"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="com.google.gdt.eclipse.designer.GXT.databinding"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="com.google.gdt.eclipse.designer.wizards"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <!-- $if eclipse.version >= 3.5$ -->
   <plugin
         id="com.google.gdt.eclipse.designer.UiBinder.wizards"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>
   <!-- $endif$ -->

   <plugin
         id="com.google.gdt.eclipse.designer.SmartGWT"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="com.google.gdt.eclipse.designer.preferences"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="com.google.gdt.eclipse.designer.launch"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>