<?xml version="1.0" encoding="UTF-8"?>
<component xmlns="http://www.eclipse.org/wb/WBPComponent">
	<description>A widget that wraps an IFRAME element, which can contain an arbitrary web site.</description>
	<!-- CREATION -->
	<creation>
		<source><![CDATA[new com.google.gwt.user.client.ui.Frame("http://www.google.com")]]></source>
		<x-attribute name="url" value="http://www.google.com"/>
	</creation>
	<!-- CONSTRUCTORS -->
	<constructors>
		<constructor>
			<parameter type="java.lang.String" property="setUrl(java.lang.String)"/>
		</constructor>
	</constructors>
	<!-- MORPHING -->
	<morphTargets>
		<morphTarget class="com.google.gwt.user.client.ui.Frame"/>
		<morphTarget class="com.google.gwt.user.client.ui.NamedFrame"/>
	</morphTargets>
</component>
