<?xml version="1.0" encoding="UTF-8"?>
<component xmlns="http://www.eclipse.org/wb/WBPComponent">
	<model class="com.google.gdt.eclipse.designer.gxt.model.widgets.menu.MenuBarItemInfo"/>
	<description>Item in MenuBar, contains child Menu.</description>
	<!-- CREATION -->
	<creation>
		<source><![CDATA[new com.extjs.gxt.ui.client.widget.menu.MenuBarItem("New MenuBarItem", null)]]></source>
	</creation>
	<!-- CONSTRUCTORS -->
	<constructors>
		<constructor>
			<parameter type="java.lang.String" property="setText(java.lang.String)"/>
			<parameter type="com.extjs.gxt.ui.client.widget.menu.Menu" child="true"/>
		</constructor>
	</constructors>
	<!-- PROPERTIES -->
	<properties-preferred names="text"/>
	<properties-advanced names=""/>
	<property-tag name="text" tag="isText" value="true"/>
	<!-- PARAMETERS -->
	<parameters>
		<!-- GEF validators -->
		<parameter name="GEF.requestValidator.child"><![CDATA[
			isComponentType(parent, "com.extjs.gxt.ui.client.widget.menu.MenuBar")
			]]></parameter>
		<!-- simple container: Component.setContextMenu -->
		<parameter name="simpleContainer.2">false</parameter>
	</parameters>
</component>
