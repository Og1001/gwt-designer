        <source><![CDATA[new com.smartgwt.client.widgets.viewer.DetailViewerField("newField", "New Field")]]></source>
    </creation>
    <!-- CONSTRUCTORS -->
    <constructors>
        <constructor>
            <parameter type="java.lang.String" property="setName(java.lang.String)"/>
        </constructor>
        <constructor>
            <parameter type="java.lang.String" property="setName(java.lang.String)"/>
            <parameter type="java.lang.String" property="setTitle(java.lang.String)"/>
        </constructor>
    </constructors>
    <!-- PROPERTIES -->
    <properties-preferred names="name title"/>
    <properties-advanced names="cellStyle image*"/>
    <properties-noDefaultValue names=""/>
    <!-- PARAMETERS -->
    <parameters>
        <parameter name="NVO.disabled">true</parameter>
    </parameters>
</component>
